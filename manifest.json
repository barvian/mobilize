{
    "name": "Mobilize",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Use mobile versions of installed web apps",
    "permissions": ["webRequest", "webRequestBlocking", "*://*/*", "management", "storage"],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
}